  },
  "homepage": "https://github.com/mtkennerly/bosca-coeil-js#readme",
  "dependencies": {
    "dialog-polyfill": "^0.5.0",
    "tone": "^13.4.9"
  },
  "devDependencies": {
    "@types/node": "^12.6.3",
    "@types/tone": "git+https://github.com/Tonejs/TypeScript.git",
    "concurrently": "^4.1.1",
    "gh-pages": "^2.0.1",
    "local-web-server": "^3.0.4",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
  }
