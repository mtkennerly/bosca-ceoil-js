    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -watch -p ./",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtkennerly/bosca-coeil-js.git"
  },
